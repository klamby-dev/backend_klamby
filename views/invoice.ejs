<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title> spurtcommerce </title>
  <link href="https://fonts.googleapis.com/css2?family=Coustard&display=swap" rel="stylesheet">
  <style type="text/css">
    body {
      font-family: 'Roboto', sans-serif;
      margin: 2mm;
    }

    * {
      box-sizing: border-box;
    }

    p,
    h1,
    h2,
    h3,
    h4,
    h5,
    h6 {
      margin: 0;
      padding: 0;
    }
  </style>
</head>

<body>

  <table width="100%" style="border-bottom: solid 3px #629d38;">
    <tr>
      <td style="text-align: left;" valign="center">
        <p style="font-size: 0.75rem;color:#000;">
          <%= orderData.setting.storeAddress %>, <%= orderData.Zone.name %>, <%= orderData.country.name %>
        </p>
        <p style="font-size: 0.75rem;color:#000;">Email : <%= orderData.setting.storeEmail %>
        </p>
        <p style="font-size: 0.75rem;color:#000;">Mobile : <%= orderData.setting.storeTelephone %>
        </p>
      </td>
      <!-- <td style="text-align: center;" width="75%" > -->
      <h2 style="font-size: 1.875rem;color: #000;text-align: center;">Invoice</h2>
      <!-- </td> -->


      <td valign="center">
        <p style="text-align: right;" width="150px">
          <img
            src="data:image/jpeg;base64,"
            width="70" height="70">
        </p>
        <p style="font-size: 0.8125rem;color: #000;text-align:right;"><span style="color: #888">Invoice No </span>:
          <%= orderData.invoiceNo %>
        </p>
        <p style="font-size: 0.8125rem;color: #000;text-align:right;"><span style="color: #888">Date </span>:
          <%= orderData.createdDate %>
        </p>
      </td>
    </tr>
    <tr>

    </tr>
  </table>

  <table width="100%" style="margin-top: 20px;">
    <tr>
      <td width="70%" valign="top">
        <h3 style="font-size: 1.0625rem;color: #000;margin-bottom: 0.6rem;">Deliver To</h3>
        <p style="font-size: 0.8125rem;color: #000;line-height: 2;">
          <%= orderData.shippingFirstname %>
            <%= orderData.shippingLastname %>
        </p>
        <p style="font-size: 0.8125rem;color: #4f4f4f;line-height: 1.2;">
          <%= orderData.shippingAddress1 %> <br>
            <%= orderData.shippingCity %>, <%= orderData.shippingCountry %><br>
                <%= orderData.shippingPostcode %>
        </p>
        <p style="margin-top: 0.8rem;margin-bottom: 1.5rem;">
          <span
            style="display: inline-block;width: 254px;height: 38px; line-height: 38px;padding: 0 5px;border-radius: 3px;background-color: #629d38;font-size: 1.1875rem;color:#fff;">
            POS : <%= orderData.paymentCity %>
          </span>
        </p>
        <p style="font-size: 0.9375rem;color: #000;line-height: 2;"><span style="color: #888">
            Order No </span>:
          <%= orderData.orderPrefixId %>
        </p>
        <p style="font-size: 0.9375rem;color: #000;line-height: 2;"><span style="color: #888">Date
          </span>:
          <%= orderData.createdDate %>
        </p>
      </td>
      <td width="" valign="top">
        <h3 style="font-size: 1.0625rem;color: #000;margin-bottom: 0.6rem;">Billing Address</h3>
        <p style="font-size: 0.8125rem;color: #000;line-height: 2;">
          <%= orderData.paymentFirstname %>
            <%= orderData.paymentLastname %>
        </p>
        <p style="font-size: 0.8125rem;color: #4f4f4f;line-height: 1.2;">
          <%= orderData.paymentAddress1 %> <br>
            <%= orderData.paymentCity %>, <%= orderData.paymentCountry %> <br>
                <%= orderData.paymentPostcode %>
        </p>
      </td>
    </tr>
  </table>

  <table width="100%" cellpadding="0" cellspacing="0" style="margin-top: 20px; border-bottom: solid 2px #000">
    <tr>
      <th
        style="font-size: 0.8125rem;padding-bottom:5px;text-align:left;border-bottom:solid 1px #ddd;font-weight: normal;color: #2b2929;">
        S.No</th>
      <th
        style="font-size: 0.8125rem;padding-bottom:5px;text-align:left;border-bottom:solid 1px #ddd;font-weight: normal;color: #2b2929;">
        Products</th>
      <th
        style="font-size: 0.8125rem;padding-bottom:5px;text-align:center;border-bottom:solid 1px #ddd;font-weight: normal;color: #2b2929;">
        Qty</th>
      <th
        style="font-size: 0.8125rem;padding-bottom:5px;text-align:right;border-bottom:solid 1px #ddd;font-weight: normal;color: #2b2929;">
        Price</th>
    </tr>
    <tr>
      <% for(var n=0; n < (orderData.productList).length ; n++) { %>
    <tr>
      <td
        style="font-size: 0.75rem;padding:10px;text-align:left;border-bottom:solid 1px #ddd;font-weight: normal;color: #000;">
        <%= n +1 %>
      </td>
      <td
        style="font-size: 0.75rem;padding:10px;text-align:left;border-bottom:solid 1px #ddd;font-weight: normal;color: #000;">
        <%= orderData.productList[n].name %> <br>
          <% if(results[n].orderOptions.length> 0){ %>
            <% for(var i=0; i < (results[n].orderOptions).length ; i++) { %>
              <%= results[n].orderOptions[i].name%>:<%= results[n].orderOptions[i].value%><br>
                  <% } %>
                    <% } %>
      </td>
      <td
        style="font-size: 0.75rem;padding:10px;text-align:center;border-bottom:solid 1px #ddd;font-weight: normal;color: #000;">
        <%= orderData.productList[n].quantity %>
      </td>
      <td
        style="font-size: 0.75rem;padding:10px;text-align:right;border-bottom:solid 1px #ddd;font-weight: normal;color: #000;background-color: #f4f4f4;">
        <%= orderData.productList[n].productPrice %>
      </td>
    </tr>
    <% } %>
      </tr>

      <tr>
        <td colspan="3" style="font-size: 0.9375rem;padding:10px;text-align:right;font-weight: normal;color: #000;">
          Grand Total</td>
        <td style="font-size: 0.9375rem;padding:10px;text-align:right;font-weight: normal;color: #000;">
          <%= orderData.total %>
        </td>
      </tr>

  </table>
  <table width="100%" cellpadding="0" cellspacing="0" style="margin-top: 20px;">
    <tr>
      <td style="font-size: 0.875rem;color: #000;">
        Grand Total in Words : <%= orderData.words %>
      </td>
    </tr>
  </table>

</body>

</html>